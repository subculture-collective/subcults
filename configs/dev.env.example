# Development environment configuration example
# Copy this file to dev.env and fill in the values

# Database
DATABASE_URL=postgres://subcults:password@localhost:5432/subcults?sslmode=disable

# Server
PORT=8080
HOST=localhost

# Environment
GO_ENV=development

# JWT Authentication
# Use a strong, random secret (minimum 32 characters recommended)
# Generate with: openssl rand -base64 32
JWT_SECRET=

# LiveKit (WebRTC)
LIVEKIT_URL=
LIVEKIT_API_KEY=
LIVEKIT_API_SECRET=

# Stripe
STRIPE_API_KEY=
STRIPE_WEBHOOK_SECRET=

# Cloudflare R2
R2_ACCESS_KEY_ID=
R2_SECRET_ACCESS_KEY=
R2_BUCKET_NAME=
R2_ENDPOINT=

# MapTiler
MAPTILER_API_KEY=
